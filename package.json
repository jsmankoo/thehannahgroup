{
  "name": "OpenShift-React-Universal-Cartidge",
  "version": "1.0.1",
  "description": "Custom Node.js cartridge for OpenShift - sample application",
  "private": true,
  "main": "start.js",
  "scripts": {
    "start": "node --use_strict start.js",
    "build": "babel ./src/ -d ./dist",
    "build:w": "babel -w ./src/ -d ./dist",
    "server": "node ./dist/server.js",
    "server:dev": "node ./devServer.js",
    "webpack": "webpack --config ./webpack.config.js",
    "compile": "npm run webpack && npm run build"
  },
  "author": "Jai S. Mankoo <jsmankoo@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "axis": "^0.6.1",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "color": "^0.11.1",
    "express": "^4.14.0",
    "jeet": "^6.1.4",
    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "morgan": "^1.7.0",
    "nuka-carousel": "^2.0.1",
    "prerender-node": "^2.3.0",
    "prismic.io": "^3.1.0",
    "pug": "^0.1.0",
    "radium": "^0.17.1",
    "raf": "^3.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^1.3.0",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.1.3",
    "redux": "^3.5.2",
    "rupture": "^0.6.1",
    "smooth-scroll": "github:cferdinandi/smooth-scroll",
    "stylus": "^0.54.5",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0"
  }
}
